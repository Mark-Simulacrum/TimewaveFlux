{
	"undef": true,
	"unused": true,
	"camelcase": true,
	"browser": true,
	"latedef": true,
	"newcap": true,
	"noempty": true,
	"quotmark": "single",
	"funcscope": true,
	"maxerr": 200,
	"browser": true,

	"predef": [
		"require",
		"module",
		"global",
		"confirm" // browser: true does not define this
	],

	"maxparams": 4,
	"maxdepth": 4,
	"maxstatements": 10,
	"maxcomplexity": 6,

	"forin": true,
	"indent": 4,
	"noarg": true,
	"nonbsp": true,
	"nonew": true,
	"notypeof": false,
	"white": true
}
